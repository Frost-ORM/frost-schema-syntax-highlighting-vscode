{
  "name": "frost-schema-syntax-highlighting",
  "displayName": "Frost Schema Syntax Highlighting",
  "description": "Syntax Highlighting for Frost Schema Language",
  "version": "0.0.1",
  "icon": "https://frost.sami-mishal.online/img/icon_small.png",
  "author": {
    "email": "samishal.1998@gmail.com",    
    "name": "Sami Mishal"
  },
  "publisher": "Frost ORM",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "fsl",
      "aliases": ["Frost Schema Language", "fsl"],
      "extensions": [".fsl","fs"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/frost-light.png",
        "dark": "./icons/frost-dark.png"
      }
    }],
    "grammars": [{
      "language": "fsl",
      "scopeName": "source.fsl",
      "path": "./syntaxes/fsl.tmLanguage.json"
    }]
  }
}
